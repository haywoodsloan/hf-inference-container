  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: HF Inference",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": ["main:app", "--reload"],
      "jinja": true,
      "env": {
        "MODEL_NAME": "haywoodsloan/ai-image-detector-dev-deploy",
        "HF_HOME": "C:\\HF_CACHE",
        "BATCH_SIZE": "8",
        "MAX_QUEUE": "40"
      }
    }
  ]
}
