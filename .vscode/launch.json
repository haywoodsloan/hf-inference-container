  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: HF Inference",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}/src",
      "args": ["main:app", "--reload"],
      "jinja": true,
      "env": {
        "MODEL_NAME": "haywoodsloan/ai-image-detector-dev-deploy",
        "TASK_TYPE": "image-classification",
        "API_KEY": "LocalDev",
        "HF_HOME": "C:\\HF_CACHE"
      }
    }
  ]
}
